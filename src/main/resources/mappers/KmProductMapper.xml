<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.kmarket.mapper.admin.KmProductMapper">

    <insert id="insertProduct"></insert>
    <select id="selectProduct" resultType="com.example.kmarket.dto.KmProductDTO"></select>
    <select id="selectProducts" resultType="com.example.kmarket.dto.KmProductDTO">
        SELECT * FROM `km_product`;
    </select>
    <update id="updateProduct"></update>
    <delete id="deleteProduct"></delete>

</mapper>